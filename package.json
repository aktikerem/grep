{
  "name": "grep-search",
  "displayName": "Grep.app Code Search",
  "description": "Instantly search through 500M+ GitHub repositories using grep.app directly from your editor",
  "version": "0.3.0",
  "publisher": "SaminAmanat",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other",
    "AI",
    "Programming Languages",
    "Education"
  ],
  "keywords": [
    "grep",
    "search",
    "github",
    "code search",
    "find code",
    "vercel",
    "repository search",
    "code finder",
    "repository finder",
    "similar code",
    "code examples"
  ],
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/amanat361/grep"
  },
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "grep.searchSelected",
        "title": "Grep: Search with Grep.app"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "grep.searchSelected",
          "group": "4_search"
        }
      ]
    },
    "keybindings": [
      {
        "command": "grep.searchSelected",
        "key": "ctrl+k g",
        "mac": "cmd+k g",
        "when": "editorHasSelection"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "esbuild": "^0.24.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
